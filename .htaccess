<IfModule mod_headers.c>
    Header set X-XSS-Protection "1; mode=block"
    Header set Referrer-Policy "same-origin"
    Header set Content-Security-Policy "default-src 'self'; script-src 'self' 'unsafe-inline' https://cdn.jsdelivr.net https://ajax.googleapis.com; style-src 'self' https://cdn.jsdelivr.net;  img-src 'self'; font-src 'self'"
</IfModule>